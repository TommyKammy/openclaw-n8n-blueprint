N8N_API_KEY=REPLACE_ME
N8N_BASE_URL=https://n8n.example.com
ONBOARDING_SETUP_LINK=https://n8n.example.com/signin

# sender account for onboarding email
GOG_ACCOUNT=oc.w.kawada@gmail.com
