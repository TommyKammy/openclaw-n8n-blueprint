# Core domain
N8N_HOST=n8n.example.com
WEBHOOK_URL=https://n8n.example.com/
GENERIC_TIMEZONE=Asia/Tokyo

# n8n + postgres
POSTGRES_DB=n8n
POSTGRES_USER=n8n
POSTGRES_PASSWORD=REPLACE_ME
N8N_ENCRYPTION_KEY=REPLACE_ME

# OpenClaw
OPENCLAW_GATEWAY_TOKEN=REPLACE_ME

